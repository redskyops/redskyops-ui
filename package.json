{
  "name": "example-frontend-react-application",
  "version": "1.0.0",
  "description": "An example frontend application built with React.",
  "main": "index.js",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "flow": "flow",
    "format": "prettier --write \"**/*.+(js|jsx|json)\"",
    "lint:js": "./node_modules/.bin/eslint ./src",
    "lint:sass": "./node_modules/.bin/sass-lint -v -q",
    "validate": "npm run lint:js && npm run format -- --list-different && npm run flow && npm run lint:sass",
    "start": "REACT_APP_BACKEND_URL=/api react-scripts start",
    "proxy": "node ./src/proxy.js",
    "test": "react-scripts test",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "watch": "echo \"Error: no watch specified\" && exit 1"
  },
  "homepage": ".",
  "keywords": [],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "body-parser": "1.19.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.4",
    "express": "4.17.1",
    "fibers": "4.0.3",
    "flow-bin": "0.116.0",
    "http-proxy-middleware": "^0.20.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.2",
    "node-fetch": "2.6.1",
    "prettier": "1.19.1",
    "request": "2.88.2",
    "sass": "1.27.0",
    "sass-lint": "1.13.1"
  },
  "dependencies": {
    "d3": "6.2.0",
    "dotenv": "8.2.0",
    "font-awesome": "^4.7.0",
    "node-sass": "^4.14.1",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "three": "0.125.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  }
}
